#include<stdio.h>
/**
 *main - print string with printf
 *
 * Description - using the main function
 * The progam will output with proper grammar, but the outcome is a piece of art,
 * Return: 0
 */
int main(void)
{
str a = "with proper grammar";
str b = "but the outcome is a piece of art,";
printf("%s,%s\n"a,b);
return (0);
}
