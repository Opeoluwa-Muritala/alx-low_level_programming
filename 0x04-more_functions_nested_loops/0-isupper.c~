#include <stdio.h>

/**
 * _isupper - Checks for uppercase
 *
 * Description - return 1 if uppercase
 * return 0 if lowercase
 *
 * RETURN: 1
 */



int _isupper(int c)
{
if (c >= 65 && c <= 90)
{
return (1);
}
else
{
return (0);
}
}

/**
 * main - run the program
 *
 * Description - return 1 if uppercase
 * return 0 if lowercase
 *
 * RETURN: 1
 */

int main(void)
{
_isupper();
}
