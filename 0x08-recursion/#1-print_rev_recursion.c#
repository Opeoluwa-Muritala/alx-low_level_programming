#include "main.h"
#include <string.h>

/**
 * _print_rev_recursion - print reverse string
 * @s: string to reverse
 * Return: 0
 */

void _print_rev_recursion(char *s)
{
char **rev
int i,j,count=0;
 while (s[count] != '\0')
   {
     count++;
   }
 j = count - 1
   for (i=0; i < count; i++)
     {
      *rev[i] = s[j];
      
     }
if (*s)
{
_putchar(*s);
_print_rev_recursion(s + 1);
}
else
{
_putchar('\n');
}
}
